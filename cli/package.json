{
  "name": "@smelly/cli",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cli.js",
  "types": "./types/src/index.d.ts",
  "dependencies": {
    "esprima": "^4.0.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/esprima": "^4.0.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.19.44",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.4",
    "nyc": "^15.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "clean": "rm -rf out/ dist/ types/",
    "compile-tests": "npm run clean && tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint . --ext ts",
    "test": "mocha -r ts-node/register ./**/**.test.ts",
    "test:watch": "nodemon --exec 'npm run test' --watch 'src/**' --ext 'ts' --delay 5",
    "test:coverage": "nyc --reporter=lcov mocha -r ts-node/register ./**/**.test.ts",
    "coveralls": "npm run test:coverage && coveralls --verbose < coverage/lcov.info",
    "cli": "ts-node bin/find-smells.ts"
  },
  "author": "",
  "license": "ISC"
}
